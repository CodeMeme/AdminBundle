<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="admin.model_container.class">CodeMeme\AdminBundle\Container\ModelContainer</parameter>
        <parameter key="admin.model_collection.class">Doctrine\Common\Collections\ArrayCollection</parameter>
    </parameters>

    <services>
        <service id="admin.model_collection" class="%admin.model_collection.class%" />
        <service id="admin.models" alias="admin.model_collection" />
    </services>
</container>
